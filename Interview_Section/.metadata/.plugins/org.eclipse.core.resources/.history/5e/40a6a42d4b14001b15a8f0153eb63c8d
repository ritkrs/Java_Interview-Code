
public class BubbleSort {
	public void bubble(int[] arr)
	{
		int temp;
		for (int i = 0; i < arr.length-1; i++) {
			for (int j = i+1; j < arr.length-i-1; j++) {
				
				if(arr[j]>arr[j+1])
				{
					temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
			}
			
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]+" ");
			
		}
	}

}
