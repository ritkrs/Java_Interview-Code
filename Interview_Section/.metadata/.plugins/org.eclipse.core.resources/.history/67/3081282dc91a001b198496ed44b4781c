package Hashmap;

import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;

public class IterateHashMap {
	public static void main(String[] args) {
		Map<Integer,String> gfg = new HashMap<String,String>(); 
    
    // enter name/url pair 
    gfg.put(1, "geeksforgeeks.org"); 
    gfg.put(2, "practice.geeksforgeeks.org"); 
    gfg.put(3, "code.geeksforgeeks.org"); 
    gfg.put(4, "quiz.geeksforgeeks.org"); 
      
    // using for-each loop for iteration over Map.entrySet() 
    for(Map.Entry<String, String> m1:gfg.entrySet())
    {
    	System.out.println("key="+m1.getKey()+" "+"values= "+m1.getValue());
    }
	}

}
